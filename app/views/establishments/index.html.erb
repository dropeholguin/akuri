<h1>Establishments</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Kind of food</th>
      <th>Website</th>
      <th>Phone</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @establishments.each do |establishment| %>
      <tr>
        <td><%= establishment.name %></td>
        <td><%= establishment.kind_of_food %></td>
        <td><%= establishment.website %></td>
        <td><%= establishment.phone %></td>
        <td><%= establishment.address %></td>
        <td><%= link_to 'Show', establishment %></td>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
